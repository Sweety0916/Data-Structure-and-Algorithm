//**************  fibonaccie seriess by one case **********************

#include <iostream>

using namespace std;
int fibonaccie(int n){
    if(n<=1)
    {return n;
    }
      
   
   else
    return fibonaccie(n-1)+fibonaccie(n-2);
   
}

int main()
{
    int n;
    cin>>n;

    cout<<fibonaccie(n)<<endl;
    return 0;
}

