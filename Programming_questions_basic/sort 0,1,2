void sort012(int arr[], int n)
    {
        int i=0;int j=n-1;int mid=0;
        while(mid<=j){
            if(arr[mid]==0){
                swap(arr[i],arr[mid]);
                i++;mid++;
            }
            else if(arr[mid]==1){
                mid++;
            }
            else{
               swap(arr[mid],arr[j]);
               j--;
              
            }
        }
    }
    
