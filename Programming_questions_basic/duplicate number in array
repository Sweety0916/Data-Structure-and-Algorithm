#include <iostream>
#include<bits/stdc++.h>

using namespace std;

void duplicate_number(int arr[],int n){
    
    // unorder_map<int,int>mp;
    vector<int>v;int ans;
     unordered_map<int,int>mp;
    for(int i=0;i<n;i++){
        mp[arr[i]]++;
    }
    for(auto i:mp){
    if(i.second>=2){
      cout<<i.first<<"->"<<i.second<<endl;
    
    }
    }
   
}

int main()
{
     int n=10;
  int arr[n]={1,1,2,3,3,3,3,2,6};

duplicate_number(arr,n);

    return 0;
}
